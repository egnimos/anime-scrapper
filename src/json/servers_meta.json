{
    "kickass_anime_server_1": {
        "server": "https://www2.kickassanime.ro",
        "server_meta": {
            "anime_listing_selector": {
                "wait_visible": "footer#footer",
                "click": "input#__BVID__50__BV_option_1_",
                "action_func": {
                    "click": "#content button.btn.btn-primary svg.svg-inline--fa.fa-chevron-right.fa-w-10"
                },
                "inner_html": "#content div.row.video-list.row.mx-0 div.col.row.video-list.row.mx-0"
            },
            "anime_listing_html_parser": {
                "find": "div.video-item.col-6.mb-2.px-1.col-2",
                "each": {
                    "find_1": {
                        "find": "a.ka-url-wrapper.video-item-poster.rounded",
                        "attror": "href"
                    },
                    "find_2": {
                        "find": "a.ka-url-wrapper.video-item-poster.rounded",
                        "attror": "style"
                    },
                    "find_3": {
                        "find": "a.ka-url-wrapper.video-item-title"
                    }
                }
            },
            "anime_info_selector": {
                "wait_visible": "footer#footer",
                "inner_html": "div#main.container-fluid.pt-3"
            },
            "anime_info_html_parser": {
                "find_1": "div#sidebar-anime-info.border.rounded.mb-3",
                "each_1": {
                    "find_1": {
                        "find": "div.poster",
                        "attror": "style"
                    },
                    "find_2": {
                        "find": "div.p-3 div.mb-2",
                        "each": {
                            "find_1": "div.font-weight-bold.mr-1",
                            "find_2": "div.mb-2 span"
                        }
                    }
                },
                "find_2": "#content div.anime-info.border.rounded.mb-3",
                "each_2": {}
            }
        }
    }
}